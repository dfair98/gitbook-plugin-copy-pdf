{
    "name": "gitbook-plugin-copy-pdf",
    "version": "0.0.1",
    "description": "Copy all pdf files to corresponding directory into the book output path",
    "engines": {
        "gitbook": "3.x.x"
    },
    "gitbook": {
        "properties": {
            "bookpath": {
                "type": "string",
                "default": "_book",
                "description": "Book output path, defaults to _book"
            }
        }
    },
    "dependencies": {
        "fs-extra": "latest"
    }
}